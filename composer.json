{
  "name": "growthcode/design-patterns-with-php",
  "description": "A project for demonstrating design patterns using PHP",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Walmir Silva",
      "email": "growthcodeoficial@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "GrowthCode\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GrowthCode\\Tests\\": "tests/"
    }
