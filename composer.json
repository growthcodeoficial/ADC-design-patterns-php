  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "repositories": [],
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
